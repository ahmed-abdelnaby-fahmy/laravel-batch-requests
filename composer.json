{
    "name": "aef/laravel-batch-requests",
    "description": "A Laravel package for efficiently handling multiple API requests in a single batch operation, reducing network overhead and improving performance for bulk operations.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelBatchRequests\\": "src/",
            "LaravelBatchRequests\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ahmed abdelnaby",
            "email": "ahmedabdelnabyfahmy1@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBatchRequests\\Providers\\BatchRequestServiceProvider"
            ],
            "aliases": {
                "BatchRequest": "LaravelBatchRequests\\Facades\\BatchRequest"
            }
        }
    }
}